{
  "name": "auto-js-doc-generator",
  "version": "1.0.25",
  "scripts": {
    "lint:check": "eslint src \"src/**/*.{ts,tsx}\"",
    "lint:write": "eslint src \"src/**/*.{ts,tsx}\" --fix",
    "commit": "git-cz",
    "changelog": "conventional-changelog -o CHANGELOG.md",
    "postcommit": "npm run changelog",
    "lerna:pack": "lerna run pack",
    "lerna:build": "lerna run build",
    "publish": "lerna publish --no-private",
    "auto:publish": "npm run lerna:build && lerna publish --no-private",
    "gen": "auto-js-doc-gen generate"
  },
  "keywords": [
    "auto",
    "auto documentation",
    "typescript",
    "documentation",
    "js doc",
    "comments",
    "javascript"
  ],
  "private": false,
  "workspaces": [
    "packages/core"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "designneexx <webmasterondesign@gmail.com>",
  "license": "ISC",
  "description": "Позволяет автоматически генерировать документацию JSDoc к вашему коду на typescript",
  "files": [
    "packages/core/dist"
  ],
  "repository": "github:designneexx/js-doc-generator",
  "devDependencies": {
    "@auto-js-doc-generator/core": "^1.1.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.8.6",
    "axios": "^1.7.9",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lerna": "^8.1.9",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.12.2"
  }
}
